<UserControl x:Class="WpfTableEditor.TableEditors.TableEditor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WpfTableEditor.TableEditors"
             xmlns:vm="clr-namespace:WpfTableEditor.TableEditors.ViewModels"
             xmlns:reogrid="clr-namespace:unvell.ReoGrid;assembly=unvell.ReoGrid"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             d:DataContext="{d:DesignInstance {x:Type vm:ITableEditorViewModel}}">
    <reogrid:ReoGridControl x:Name="reoGridControl"/>
</UserControl>
